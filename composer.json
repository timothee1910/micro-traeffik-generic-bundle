{
    "name": "timothee1910/micro-traeffik-generic-bundle",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.2",
        "symfony/console": "6.2.*",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client-contracts": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.50",
        "symfony/yaml": "6.2.*",
        "doctrine/orm": "^2.15"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Micro\\TraeffikGenericBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
